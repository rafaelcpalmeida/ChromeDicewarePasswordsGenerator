{
    "manifest_version": 2,
    "name": "Diceware passwords generator",
    "description": "Generate your secure passwords using diceware algorithm",
    "version": "1.0.2",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/avl.min.js", "js/dicewareWords.js", "js/dicewareGenerator.js", "js/dicewareGeneratorBackground.js"],
        "persistent": true
    }
}
